qualif TabIdx(v:int) : v <= 10
qualif TabIdx(v:int) : v < 10
qualif TabOrNeg(v:int) : v != -1 => &&[v >= 0; v < 10]

qualif TagsetAdd(v:ptr, t:int): TAGSET([v]) = Set_cup([TAGSET([t]); Set_sng([t])])
qualif TagsetAddFld8(v:ptr, t:int): TAGSET([DEREF([v + 8])]) = Set_cup([TAGSET([t]); Set_sng([t])])

qualif TagsetSng(v:ptr, t:int): TAGSET([v]) =  Set_sng([t])
qualif TagsetSngFld4(v:ptr, t:int): TAGSET([(DEREF([v + 4]) : int)]) = Set_sng([t])
qualif TagsetSngFld8(v:ptr, t:int): TAGSET([DEREF([v + 8])]) = Set_sng([t])

qualif IdxOfFd(v:int, p:ptr): v = DEREF([p])

qualif FldEq(v:ptr, a:int): (DEREF([v + 4]) : int) = a


qualif MsgPolicyType(v:ptr): TAGSET([v]) = TAGSET([(DEREF([V + 4]) : int)])
qualif MsgPolicyContent(v:ptr): TAGSET([DEREF([v + 8])]) = TAGSET([(DEREF([V + 4]) : int)])

qualif TagsEq0(v:ptr, p:ptr): TAGSET([v]) = TAGSET([p])
